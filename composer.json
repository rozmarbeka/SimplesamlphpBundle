{
    "name": "rozmarbeka/simplesamlphp-bundle",
    "type": "symfony-bundle",
    "description": "SimpleSAMLphp Bundle for Symfony2",
    "keywords": ["simplesamlphp", "saml"],
    "authors": [
        {
            "name": "hslavich",
            "email": "hernan.slavich@gmail.com"
        },
        {
            "name": "Moritz Hesse",
            "email": "moritz.hesse@tu-dresden.de"
        }
    ],
    "repositories": [
      {
        "type": "package",
        "package": {
          "name": "rozmarbeka/simplesamlphp",
          "version": "dev-master",
          "source": {
            "url": "https://github.com/rozmarbeka/simplesamlphp",
            "type": "git",
            "reference": "origin/master"
          }
        }
      }
    ],
    "require": {
        "symfony/symfony": ">=2.8",
        "rozmarbeka/simplesamlphp": "dev-master"
    },
    "autoload": {
        "psr-4": { "Rozmarbeka\\SimplesamlphpBundle\\": "" }
    }
}
